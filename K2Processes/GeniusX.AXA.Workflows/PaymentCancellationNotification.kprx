                && string.IsNullOrEmpty(K2.Configuration.MobileUrl))
            {
                K2.AddWorklist("ASP", "");
                e.Result = true;
            }
            else
            {
                if (!AllowAnyUserToFinishEventDone)
                {
                    if (Convert.ToBoolean(K2.Configuration.AllowAnyUserToFinishEvent))

                        this.XmlHelper.XMLFields[name, scope].XPaths[xpath].Values.Add(value.ToString().ToLower());
                        break;
                    default:
                        this.XmlHelper.XMLFields[name, scope].XPaths[xpath].Values.Add(value.ToString());
                        break;
                }

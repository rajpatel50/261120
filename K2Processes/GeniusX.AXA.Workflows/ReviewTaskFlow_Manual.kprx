                            if (list[0] is XmlAttribute)
                            {
                                (list[0] as XmlAttribute).OwnerElement.ParentNode.AppendChild((list[0] as XmlAttribute).OwnerElement.CloneNode(true));
                            }
                            if (list[0] is XmlElement)

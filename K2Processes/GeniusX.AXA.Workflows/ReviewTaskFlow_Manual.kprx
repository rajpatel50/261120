                xmldoc.Schemas.Add(schemaset);
                xmldoc.LoadXml(xmlValue);

                if (xmldoc.SelectNodes(this.XPath).Count != 0)
                {
                    while (xmldoc.SelectNodes(this.XPath).Count != this.Values.Count && xmldoc.SelectNodes(this.XPath).Count != 0)

{
	public partial class DestinationRule_3b24c667b112477097c86aed25c376da : IWorkflowContext<hostContext>
	{
		#region K2 Context

        private hostContext _k2;

        #endregion

		#region IWorkflowContext<hostContext> Members

        public Project_43255b4c72a141038c4baad6ea7ead45.DestinationRuleContext_3b24c667b112477097c86aed25c376da K2
        {
            get

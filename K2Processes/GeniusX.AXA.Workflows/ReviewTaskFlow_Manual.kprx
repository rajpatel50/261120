                    if (listNodes[i] is XmlAttribute)
                    {
                        listNodes[i].Value = this.Values[i];
                    }
                    if (listNodes[i] is XmlElement)
                    {
                        (listNodes[i] as XmlElement).InnerText = this.Values[i];
                    }
                }

                retval = xmldoc.OuterXml;

                xmldoc = null;

                return retval;
            }

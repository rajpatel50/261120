                string schemaUri = string.Empty;
                switch (scope)
                {
                    case K2Scope.Process:
                        xmlValue = K2.ProcessInstance.XmlFields[name].Value;
                        schemaUri = K2.ProcessInstance.XmlFields[name].Schema;

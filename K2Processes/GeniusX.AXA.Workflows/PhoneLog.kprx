        {
            #region Constructor
            public XMLHelper_XPath()
            {

            }
            public XMLHelper_XPath(string xpath)
                : this()
            {
                this.XPath = xpath;
            }
            #endregion

            #region Variables
            private string _xpath;
            private List<string> _values;
            #endregion

            #region Properties
            public string XPath
            {
                get
                {

                    throw Ex;
                }
                finally
                {
                    xmlserializer = null;
                    memorystream = null;
                    xmlserializernamespaces = null;
                    streamreader = null;
                    xmldocument = null;
                }

                using (System.IO.TextWriter tw = new System.IO.StreamWriter(filename))
                {
                    tw.WriteLine(returnval);

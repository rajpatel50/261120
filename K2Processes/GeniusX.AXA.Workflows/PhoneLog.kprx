            public void InitXml(XmlDocument xmldoc, XmlDocument initdoc)
            {
                EnsureXmlValue();

                xmldoc.LoadXml(this.XMLValue);
                xmldoc.Schemas.Add(this.SchemaSet);
                initdoc.LoadXml(this.XMLValue);

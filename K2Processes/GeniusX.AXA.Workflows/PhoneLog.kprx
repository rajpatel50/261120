            K2Scope scope = (K2Scope)Enum.Parse(typeof(K2Scope), GetK2Configuration(CollectionType.ReturnValueXmlFieldScope, setIdx).ToString());
            
            if (!this.XmlHelper.XMLFields.Contains(name, scope))
            {

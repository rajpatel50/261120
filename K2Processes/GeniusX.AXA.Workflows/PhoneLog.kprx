                }

                XmlNodeList listNodes = xmldoc.SelectNodes(this.XPath);
                for (int i = 0; i <= this.Values.Count - 1; i++)
                {
                    if (listNodes[i] is XmlAttribute)
                    {
                        listNodes[i].Value = this.Values[i];

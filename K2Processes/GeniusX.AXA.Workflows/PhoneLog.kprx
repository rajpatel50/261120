            public void Validate()
            {
                XmlDocument xmldoc = new XmlDocument();
                try
                {
                    xmldoc.LoadXml(this.XMLValue);
                    xmldoc.Schemas.Add(this.SchemaSet);

                    xmldoc.Validate(new ValidationEventHandler(XmlDocumentValidation));
                }
                finally
                {
                    xmldoc = null;

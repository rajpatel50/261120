                {
                    SourceCode.Workflow.Common.Xml.XmlSampleGenerator sampleGen = new SourceCode.Workflow.Common.Xml.XmlSampleGenerator(this.SchemaSet, XmlQualifiedName.Empty);
                    sampleGen.MaxThreshold = 1;

